{
  "name": "@ractivejs/loader-rollup",
  "version": "0.10.0-edge",
  "files": [
    "dist"
  ],
  "main": "dist/lib.js",
  "module": "dist/lib.mjs",
  "peerDependencies": {
    "@ractivejs/core": "1.0.0"
  },
  "scripts": {
    "make": "rollup -c src/rollup.config.js",
    "test": "rollup -c test/rollup.config.js && nyc qunit tmp/test.js"
  },
  "nyc": {
    "temp-directory": "./coverage/.nyc_output",
    "include": [ "src/**/*.js" ],
    "report-dir": "coverage/node",
    "reporter": [ "json", "html" ]
  }
}
