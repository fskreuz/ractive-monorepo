[
  {
    "rel": "component",
    "type": "ractive/component",
    "href": "./path/to/MyComponent.ractive.html",
    "id": "MyComponent"
  },
  {
    "rel": "partial",
    "type": "ractive/partial",
    "href": "./path/to/MyPartial.html",
    "id": "MyPartial"
  },
  {
    "rel": "style",
    "type": "ractive/style",
    "href": "./path/to/MyStyle.css",
    "id": "MyStyle"
  },
  {
    "rel": "style",
    "type": "ractive/script",
    "href": "./path/to/MyScriptStyle.js",
    "id": "MyScriptStyle"
  },
  {
    "rel": "template",
    "type": "ractive/template",
    "content": "\n  <h1>Hello, {{ user }}!</h1>\n  <p>Answer to the Ultimate Question of Life, the Universe, and Everything: {{ answer }}</p>\n  <div>Expression {{ a + b }}</div>\n  <MyComponent />\n  {{> MyPartial }}\n"
  },
  {
    "rel": "partial",
    "type": "ractive/partial",
    "content": "\n  <span>Hello, World!</span>\n",
    "id": "MyPartial"
  },
  {
    "rel": "style",
    "type": "ractive/style",
    "content": "\n  h1 {\n    color: red\n  }\n"
  },
  {
    "rel": "style",
    "type": "ractive/script",
    "content": "\n  return `\n    p {\n      color: ${data.color}\n    }\n  `\n"
  },
  {
    "rel": "script",
    "type": "ractive/script",
    "content": "\n  const answer = require('hhgttg')\n\n  component.exports = {\n    data: {\n      user: 'World',\n      answer: answer,\n      a: 1,\n      b: 2\n    }\n  }\n"
  }
]
